{
  "name": "moderntribe/tribe-common",
  "description": "Common classes and functions used in our plugins",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "events"
  ],
  "homepage": "http://tri.be/shop/wordpress-events-calendar/",
  "license": "GPL-2.0",
  "require-dev": {
    "lucatume/wp-browser": "^2.2.4",
    "codeception/codeception": "^2.5.5",
    "phpunit/phpunit": "^6.5.14",
    "lucatume/function-mocker-le": "^1.0",
    "wp-cli/checksum-command": "1.0.5",
    "moderntribe/tribe-common-styles": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "name": "moderntribe/tribe-common-styles",
      "type": "github",
      "url": "https://github.com/moderntribe/tribe-common-styles",
      "no-api": true
    }
  ],
  "require": {
    "lucatume/di52": "~2.0.10",
    "firebase/php-jwt": "~5.0.0"
  },
  "extra": {
    "installer-paths": {
      "src/resources/postcss/utilities": [
        "moderntribe/tribe-common-styles"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  }
}
