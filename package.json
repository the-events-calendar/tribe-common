{
  "name": "tribe-common",
  "version": "7.0.0",
  "repository": "git@github.com:the-events-calendar/tribe-common.git",
  "_resourcepath": "src/resources",
  "_domainPath": "lang",
  "_textDomain": "tribe-common",
  "_glotPressUrl": "https://translations.theeventscalendar.com",
  "_glotPressSlug": "tribe-common",
  "_glotPressFileFormat": "%textdomain%-%wp_locale%.%format%",
  "_glotPressFormats": [
    "po",
    "mo"
  ],
  "_glotPressFilter": {
    "translation_sets": false,
    "minimum_percentage": 30,
    "waiting_strings": false
  },
  "_filePath": {
    "jest": [
      "src/modules/**/__tests__/**/*.js"
    ]
  },
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "scripts": {
    "lint:js": "wp-scripts lint-js src/",
    "lint:css": "wp-scripts lint-style",
    "lint": "npm run lint:js && npm run lint:css",
    "format:css": "wp-scripts lint-style --fix",
    "format:js": "wp-scripts lint-js src/ --fix",
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "dev": "WP_DEVTOOL=eval-source-map wp-scripts start",
    "test": "jest",
    "changelog": "./vendor/bin/changelogger add",
    "classy:format": "wp-scripts format src/resources/packages/classy tests/classy_jest && composer run classy:format",
    "classy:jest": "jest --config tests/classy_jest/jest.config.js tests/classy_jest",
    "classy:test": "npm run classy:jest && slic run classy_integration",
    "classy:pre-commit": "npm run classy:format && npm run classy:test"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "@moderntribe/common": "file:src/modules",
    "@nfen/redux-reducer-injector": "0.0.3",
    "@redux-devtools/extension": "^3.3.0",
    "@wordpress/a11y": "^4.11.0",
    "@wordpress/data": "^10.11.0",
    "@wordpress/i18n": "^5.11.0",
    "@wordpress/is-shallow-equal": "^5.11.0",
    "@wordpress/url": "^4.11.0",
    "bezier-easing": "^2.1.0",
    "chrono-node": "^2.7.7",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.10",
    "file-loader": "^6.2.0",
    "he": "^1.2.0",
    "jquery": "3.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "php-date-formatter": "^1.3.6",
    "prop-types": "^15.6.2",
    "querystringify": "^2.0.0",
    "react-18-input-autosize": "^3.0.0",
    "react-day-picker": "^9.3.0",
    "react-places-autocomplete": "^6.1.2",
    "react-redux": "^9.1.2",
    "react-scroll-to": "^1.2.2",
    "react-select": "^5.8.3",
    "react-test-renderer": "^18.3.1",
    "react-text-input": "^0.0.8",
    "react-textarea-autosize": "^8.5.5",
    "redux": "^5.0.1",
    "redux-saga": "^0.16.1",
    "redux-thunk": "^3.1.0",
    "reselect": "^3.0.1",
    "uniqid": "^5.0.3",
    "validator": "^13.7.0",
    "whatwg-fetch": "^2.0.4",
    "zero-fill": "^2.2.3"
  },
  "devDependencies": {
    "@emotion/jest": "^11.13.0",
    "@jest/globals": "^29.7.0",
    "@stellarwp/tyson": "*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@wordpress/block-editor": "^14.15.0",
    "@wordpress/components": "^29.9.0",
    "@wordpress/date": "^5.23.0",
    "@wordpress/editor": "^14.19.0",
    "@wordpress/element": "^6.19.0",
    "@wordpress/hooks": "^1.3.2",
    "@wordpress/i18n": "^5.19.0",
    "@wordpress/plugins": "^7.19.0",
    "@wordpress/scripts": "^30.14.1",
    "cross-env": "^5.2.0",
    "enzyme-to-json": "^3.6.1",
    "file-loader": "^6.2.0",
    "gettext-parser": "1.3.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "postcss-custom-media": "^11.0.5",
    "postcss-import": "^16.1.0",
    "postcss-mixins": "^11.0.3",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redux-mock-store": "^1.5.3",
    "stylelint-order": "^6.0.4",
    "ts-jest": "^29.2.6",
    "webpack-merge": "^4.1.4"
  },
  "overrides": {
    "babel-plugin-lodash": {
      "@babel/types": "~7.20.0"
    }
  }
}
